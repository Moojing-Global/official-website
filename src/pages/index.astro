---
import MainLayout from "@layouts/MainLayout.astro";
import { Icon } from "astro-icon/components";

const title = "Coming Soon";
const description = "Our website is coming soon. Stay tuned for updates!";
const canonical = "/";
---

<MainLayout {title} {description} {canonical}>
